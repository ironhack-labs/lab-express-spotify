<div class="container p-5">
  <table class="table">
    <thead>
      <tr class="table-dark">
        <th>Title</th>
        <th>Listen</th>
      </tr>
    </thead>
    <tbody>
      {{#each tracks}}
        <tr>
          <td>{{name}}</td>
          <td>
            <audio controls>
              <source src="{{preview_url}}" />
            </audio></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>