<link rel="stylesheet" type="text/css" href="\styles\tracks.css">
<table>
    <tr>
        <th class="title-column">Title</th>
        <th class="listen-column">Listen</th>
    </tr>
    {{#each tracks}}
    <tr>
        <td>{{name}}</td>
        <td class="audio-controls">
            <audio controls>
                <source src="{{preview_url}}" type="audio/mpeg">
                Your browser does not support the audio element.
            </audio>
        </td>
    </tr>
    {{/each}}
</table>