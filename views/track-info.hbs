<section id="tracks">
    <h1> title </h1>
    <h1> listen </h1>

<div class="album-item">
    {{#each tracks}}
    <h2>{{name}}</h2>
        {{#if preview_url}}
            <audio controls src="{{preview_url}}">
            <a href="{{preview_url}}"></a>
            </audio>
        {{/if}}
</div> 
        {{else}} 
        <p>Tracks not found</p>  
        {{/each}}

</section>
