    <div class="album">
        <span class="artist-names-album"><p>{{#each this.artists}}{{#if @first}}<b> {{this.name}} </b>
            {{else}} - {{this.name}}{{/if}}{{/each}}</p></span>
            {{#if this.images}}
            <img src="{{this.images.0.url}}"
            {{else}}
             <img src="/images/deafault-pic.png"
            {{/if}}
            alt="{{this.name}}">
        <h2>{{this.name}}</h2>
        <a href="/view-tracks/{{this.id}}">View Tracks</a>
    </div>