<div class="row albumsRow">
    {{#each artistAlbums}}
    <div class="col-3 albumsCards">

        <div class="card" style="width: 18rem;">
            <img src="{{images.[0].url}}" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{name}}</h5>
                <p class="card-text"></p>
                <a href="/tracks/{{id}}" class="btn btn-primary">View songs</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>