<h1>Tracks of {{this.artists.0.name}} </h1>
<div class="artist-container">
    {{#each trackList}}
        <div class="item-info">
            <h3>{{this.name}}</h3>
            <audio controls>
            <source src="{{this.preview_url}}" type="audio/ogg">
            <source src="{{this.preview_url}}" type="audio/mpeg">
            </audio>
        </div>
    {{/each}}
</div>