<header id="tracks-header">
  <ul>
    <li class="first-col">Title</li>
    <li class="second-col">Listen</li>
  </ul>
</header>

<main id="tracks">
  {{#each items}}
    <div class="track">
      <span class="first-col">{{this.name}}</span>
      <span class="second-col">00000</span>
    </div>
  {{/each}}
</main>