<div class="container">
    {{#each items}}
        <div id="tracks">
            <div>{{this.name}}</div>
            <audio controls src="{{this.preview_url}}"> Your browser does not support the<code>audio</code> element.</audio>
            <div><a href="/tracks/{{this.id}}" id="viewTracksBtn" class="btn">View albums</a></div>
        </div>
    {{/each}}
</div>

