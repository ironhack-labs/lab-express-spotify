<table class="table tracks-table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Title</th>
            <th scope="col">Listen</th>
        </tr>
    </thead>
    <tbody>
        {{#each tracks}}
        <tr>
            <th scope="row">{{name}}</th>
            <td><audio controls src="{{preview_url}}"></audio></td>
        </tr>
        {{/each}}
    </tbody>
</table>