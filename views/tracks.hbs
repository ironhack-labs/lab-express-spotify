<h1 class="heading">Tracks of {{albumName}}</h1>
<div id="tracksContent">
    <table id="tracksTable" border="1">
    <thead>
        <tr>
            <th>Track Name</th>
            <th>Preview</th>
        </tr>
    </thead>
    <tbody>
        {{#each tracks}}
        <tr class="tracksItem">
            <td>{{name}}</td>
            <td>
                <audio class="tracksAudio" controls src="{{preview_url}}"></audio>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>

</div>
