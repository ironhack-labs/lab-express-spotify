<div id="trackHeader">
    <p>Title</p>
    <p>Listen</p>
</div>

{{#if tracks}}

<ul id="tracksList">
    {{#each tracks}}
        <li>
            <p>{{this.name}}</p>
            <audio controls src="{{this.preview_url}}">
                Your browser does not support the
                <code>audio</code> element.
            </audio>
        </li>
    {{/each}}
</ul>
 {{else}}

 <p id="noTracks">Sorry, no tracks here....</p>

{{/if}}
