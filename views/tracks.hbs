<nav>
    <a href="/">New Search</a>
</nav>

<h1>Album Tracks</h1>

<section id="tracks-results">
    
    <ol>
        {{#each tracksData.body.items}}
        <li>{{this.name}}</li>
            <audio
            controls
            src="{{this.preview_url}}">
                <a href="{{this.preview_url}}">
                    Play audio
                </a>
            </audio>
        {{/each}}
    </ol>
</section>