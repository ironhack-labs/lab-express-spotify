<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link rel='stylesheet' href='/styles/style.css' />
    <title>Tracks</title>
  </head>
  <body>
    <div class='header'>
      <a href='/'><i class='fa-solid fa-house fa-2xl'></i></a>
      <h1>Tracks</h1>
      {{! <a href='/albums/:artistId'>Back</a> }}
    </div>
    <div class='bg-grey'>
      <div class='div-all-tracks'>
        {{#each albumTracks}}
          <div>
            <h1 class='search-tracks-heading'>{{this.name}}</h1>
            <audio controls src='{{this.preview_url}}'>
              <a href=''> </a>
            </audio>
          </div>
        {{/each}}
      </div>
    </div>
  </body>
</html>