<h1>Pistes de l'album</h1>

<ul>
  {{#each tracks}}
    <li>
      <div class="trackcountainer">
      <h3>{{this.name}}</h3>
      {{#if this.preview_url}}
        <audio controls>
          <source src="{{this.preview_url}}" type="audio/mpeg">
          Votre navigateur ne supporte pas l'élément audio.
        </audio>
      </div>
      {{/if}}
    </li>
  {{/each}}
</ul>