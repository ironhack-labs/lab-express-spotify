<ul>
  {{#each tracks}}
    <li>
      <h4>{{this.name}}</h4>
      <br />
      {{#if this.preview_url}}
        <audio controls>
          <source src="{{this.preview_url}}" type="audio/mpeg" />
        </audio>
      {{else}}
        <p><b>No hay preview disponible</b></p>
      {{/if}}
    </li>
  {{/each}}
</ul>