<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/styles/style.css" />
  </head>
  <body>
    
      <div class="searched-tracks-title-listen">
        <h1>Title</h1>
        <h1>Listen</h1>
      </div>
      <div class="searched-tracks"></div>
      {{#each searchedTracks}}
        <ul class="searched-tracks-ul">
          <li class="searched-tracks-ul-li">
            <h1 class="searched-tracks-heading">{{this.name}}</h1>
            <audio class="searched-tracks-audio" controls src="{{this.preview_url}}">
              <a href="{{this.preview_url}}"> </a>
            </audio>
          </li>
        </ul>

      {{/each}}
    </div>
  </body>
</html>