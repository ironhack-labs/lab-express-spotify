<div class="title">
    <h1>Tracks</h1>
</div>
<div class="content">
    <div class="track-container">
        {{#each tracks }}
        <div class="track-card">
            <h6>{{name}}</h6>
            <audio class="audio-controls" controls src={{preview_url}}></audio>
        </div>
        {{/each}}
    </div>
</div>

