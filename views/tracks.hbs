<div class="container">
    <div class="row header-row">
            <span class="col-lg-8">Title</span>
            <span class="col-lg-4">Listen</span>
        </div>
    {{#each tracksList}}
        <div class="row track-item">
            <div class="col-lg-8">
                <p>{{this.name}}</p>
            </div>
            <div class="col-lg-4">
                <audio controls src={{this.preview_url}}></audio>
            </div>
        </div>
    {{else}}
        {{err}}
    {{/each}}
</div>