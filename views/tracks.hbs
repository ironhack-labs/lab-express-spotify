<div id="trackList">
    {{#each items}}
    <div class="track">
        <div>{{this.name}}</div>
        <div>
            {{#if this.preview_url}}
            <audio controls>
                <source src="{{this.preview_url}}" type="audio/mp3">
            </audio>
            {{else}}
            Preview not available
            {{/if}}
        </div>
    </div>
    {{/each}}
</div>