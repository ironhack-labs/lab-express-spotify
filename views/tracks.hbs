<section class="tracks">
    <div class="tracks-div">
        <p>Title</p>
        <p id="listen">Listen</p>
    </div>
    {{#each tracks}}
    <article class="track">
        <h2>{{this.name}}</h2>
        <audio controls src="{{this.preview_url}}">
            Your browser does not support the
            <code>audio</code> element.
        </audio>
    </article>
    {{/each}}
</section>