<div id="tracks-wrapper">
    {{#each items}}
        <div id="track">
            <div id="title">
                <h2>{{this.name}}</h2>
            </div>

            <div id="audio">
                <audio
                    controls
                    src="{{this.preview_url}}">
                        Your browser does not support the
                        <code>audio</code> element.
                </audio>
            </div>
        </div> 
    {{/each}}
</div>
