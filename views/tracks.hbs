    <table class="table">
        <thead>
            <tr class="table-dark">
                <th >Title</th>
                <th>Listen</th>
            </tr>
        </thead>
        <tbody>
            {{#each tracks}}
            <tr class="table-secondary">
                <td >{{this.name}}</td>
                <td class="d-flex justify-content-end"><audio controls src="{{this.preview_url}}"></audio></td>
            </tr>
            {{/each}}
        </tbody>
    </table>