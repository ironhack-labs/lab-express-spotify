<h1>PRUEBA DE TRACKS</h1>


<div class="row">

    {{#each data.body.items}}

    <div class="card" style="width: 18rem;">
        <h5 class="card-title"> {{this.name}}</h5>
        <audio controls>
            <source src="{{this.preview_url}}" type="audio/mpeg">
            Your browser does not support the audio element.
        </audio>
    </div>
    {{/each}}
</div>