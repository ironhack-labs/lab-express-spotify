<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Tracks</title>
  </head>
  <body>

    {{#each tracks}}
      <h1>{{this.name}}</h1>
      <audio controls src="{{this.preview_url}}">
        Your browser does not support the
        <code>audio</code>
        element.
      </audio>
    {{/each}}

  </body>
</html>