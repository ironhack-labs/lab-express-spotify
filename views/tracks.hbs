<div class="tracks-background-div">
  <div class="tracks-header-div"><p>Title</p><p>Listen</p></div>
  {{#each tracks}}
    <div class="track-div">
      <p>{{this.name}}</p>
      <audio controls><source src="{{this.preview_url}}" type="audio/mpeg" />Not
        supported in your browser</audio>
    </div>
  {{/each}}
</div>