<div class='result-page'>
    <h2>Album Tracks</h2>
    <a class="back-btn" href="javascript:history.back()">Back</a>
    <table>
    <tr>
        <th>Title</th>
        <th>Listen</th>
    </tr>
    {{#each tracks}}
    <tr>
        <td>{{this.name}}</td>
        <td>
            <audio controls>
            <source src='{{this.preview_url}}' type='audio/wav'>
            </audio>
        </td>
    </tr>
    {{/each}}
    </table>
</div>

