<div class="container">
    <div class="row">
        <div class="col-12 d-flex justify-content-between mt-2 pt-2 bg-dark align-items-center text-white fw-bold">
            <p>Title</p>
            <p class="me-5">Listen</p>
        </div>
        {{#each data}}
        <div class="col-12 d-flex justify-content-between mt-2 pt-2 bg-light align-items-center fw-bold">
            <p>{{name}}</p>
            <audio controls src="{{preview_url}}"></audio>
        </div>
        {{/each}}
    </div>
</div>