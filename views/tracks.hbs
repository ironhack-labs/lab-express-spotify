<div class="tracks-container">
    <div class="tracks-header">
        <h2>Tracks</h2>
        <h2>Listen to preview:</h2>
    </div>
 {{#each tracksInfo }}
    <div class="tracks-item">
        <div class="track">
            <p>{{this.name}}</p>
        </div>
    <figure>
        <audio controls src="{{this.preview_url}}">
            <a href="{{this.preview_url}}"></a>
        </audio>
    </figure>
    </div>
{{/each}}
</div>


