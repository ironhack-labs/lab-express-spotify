<header id="tracks-header">
  <ul>
    <li class="first-col">Title</li>
    <li class="second-col">Listen</li>
  </ul>
</header>

<main id="tracks">
  {{#each items}}
    <div class="track">
      <span class="first-col">{{this.name}}</span>
      <span class="second-col">
        <audio controls>
          <source src="{{this.preview_url}}" type="audio/mpeg">
        </audio>
      </span>
    </div>
  {{/each}}
</main>