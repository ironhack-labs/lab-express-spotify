<div class="tracks">
  {{#each this.tracks}}
    <div class="tracks">
      <ul>
        <li><p class="trackTitle">{{this.name}}</p></li>
        <li><audio controls src="{{this.preview_url}}">
            <a href="{{this.preview_url}}"></a>
          </audio></li>

      </ul>

    </div>
  {{/each}}
</div>