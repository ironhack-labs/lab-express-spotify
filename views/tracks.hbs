{{#each tracksArray}}
	<div method="GET" action="/albums/{{this.id}}">

		<div class="header-container">
			<h2>Title</h2>
			<h2>Listen</h2>
		</div>

		<div class="track-description-container">

			<h3>{{this.name}}</h3>

			<audio controls src="{{this.preview_url}}">
				<a href="{{this.preview_url}}">
					Download audio
				</a>
			</audio>

		</div>
	</div>
{{/each}}