<div class="trackDiv">
  <nav>
    <ul>
      <li>Title</li>
      <li>Listen</li>
    </ul>

  </nav>
  {{#each tracks}}
    <section class="trackSection">
      <h3>{{name}}</h3>
      <audio controls src="{{this.preview_url}}">
        Your browser does not support the
        <code>audio</code>
        element.
      </audio>

    </section>

  {{/each}}

</div>