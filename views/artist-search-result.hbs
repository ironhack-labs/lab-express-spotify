<div id="artistPage">
    {{#each artists}}

<div id="artistDetails">

    <p id="artistTitle"> {{this.name}}</p>



    <img src="{{this.images.1.url}}" id="artistImage"> </img>

    <div></div>

    <a id="artistButton" href="/albums/{{this.id}}"> View albums </a>

  </div>
    {{/each}}
  
</div>

