


<section id="container">

{{#each artists.items}}
<div class="artist-item">

{{#if images}}

    <img src="{{images.0.url}}" alt="image-here">

{{else}}

        <p>Image not available</p>

{{/if}}

    <span>{{name}}</span>
    <a href="/albums/{{id}}">View Albums</a>

</div>  
{{/each}}
</section>

