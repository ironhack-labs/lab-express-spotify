<h1>Results:</h1>
<section class="results">
  {{#each results}}
    <div class="resultBox">
      <h1>{{this.name}}</h1>
      <img src="{{this.images.1.url}}" alt="{{this.name}}" />
      <form method="GET" action="/albums/{{this.id}}">
        <button type="submit">View Albums</button>
      </form>
    </div>
  {{/each}}
</section>