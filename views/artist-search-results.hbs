<div class="row artistRow">
    {{#each artistInfo}}
    <div class="col-3 artistCards">

        <div class="card" style="width: 18rem;">
            <img src="{{images.[0].url}}" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{name}}</h5>
                <p class="card-text"></p>
                <a href="/albums/{{id}}" class="btn btn-primary">View albums</a>
            </div>
        </div>


    </div>
    {{/each}}
</div>