{{#each artistDetails}}
<div>
    <div>
        <img src="{{this.images}}" alt="{{this.name}}">
    </div>
    <div>
      <h2>{{this.name}}</h2>
      <a class="btn " href="{{this.external_urls.spotify}}">Go to {{this.name}}</a>
      <a href="/albums/{{this.id}}">View Albums</a>
    </div>
</div>
{{/each}}