<div class="container">
    <h1>Search Results</h1>
    <div id="artists">
        {{#each artists}}
        <div class="card">
            <img src="{{this.images.0.url}}" alt="{{this.name}}">
            <h2>{{this.name}}</h2>
            <a href="/albums/{{this.id}}">View Albums</a>
        </div>
        {{/each}}
    </div>
</div>