<div class="container">
    <div class="row">
        {{#each artist}}
        <div class="col-4">
            <p class="namesArtists">{{name}}</p>

            <p> <img src="{{images.0.url}}" alt="photoArtist" class="imgResults"></p>

            <a href="/albums/{{id}}" class="btn btn-warning viewButtons"> View albums </a>
        </div>
        {{/each}}
    </div>
</div>