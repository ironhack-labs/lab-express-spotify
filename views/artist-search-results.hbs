<h1>Artists:</h1>
<div class="card-container">
{{#each artist}}
<div class="card">
<h1>{{this.name}}</h1>
    <img src="{{this.images.0.url}}" alt="{{this.name}}">
    <form action="/albums/{{this.id}}" method="GET">
        <button type="submit" class="btn">View Albums</button>
    </form>
</div>
{{/each}}
</div>
