<div class="container">
    <div class="row">
        {{#each data.body.artists.items}}
        <div class="col-md-4">
            <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="{{images.[0].url}}" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">{{name}}</h5>
                <a href="/albums/{{id}}" class="btn btn-primary">View Albums</a>
            </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>