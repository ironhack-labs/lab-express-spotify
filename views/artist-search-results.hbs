<main id="artist-search-results">
  {{#each items}}
  <div class="artist">
    {{#if this.images.[0].url}}
    <img src="{{this.images.[0].url}}" />
    {{else}}
    <img src="https://www.designshock.com/wp-content/uploads/2016/04/man-4-400.jpg" />
    {{/if}}
    <div class="artist-info">
      <h2>{{this.name}}</h2>
      <a class="button" href="/albums/{{this.id}}">View Albums</a>
    </div>
  </div>
  {{/each}}
</main>