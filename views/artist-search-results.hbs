<div class="container row">
  {{#each data}}
      <div class="card col-sm-4">
        <img class="card-img-top" src="{{images.[0].url}}" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title">{{name}}</h5>
          <a href="/albums/{{id}}" class="btn">View Albums</a>
        </div>
      </div>    
    {{/each}}  
</div>

