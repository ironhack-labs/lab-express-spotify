{{!-- {{log "received" this}} --}}

<div id="artist-list">

    {{#each this.artists.items}}
        {{!-- {{log this}} --}}
        <div class="artist">
            {{log "artist" this}}
            {{#if this.images}}
                <img src="{{this.images.[0].url}}" alt="{{this.name}}">
            {{else}}
                <img src="/images/missing.png" alt="{{this.name}}" width="500px">
            {{/if}}
            <span>{{this.name}}</span>
            <a href="/albums/{{this.id}}">View Albums</a>

        </div>
        {{!-- {{this.name}} --}}
    {{/each}}

</div>