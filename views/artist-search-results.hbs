<ul class="artist-grid">
  {{#each artists}}
    <li class="artist-item">
     
      {{#if this.images.[0]}}
        <img src="{{this.images.[0].url}}" alt="{{this.name}}" class="artist-image">
      {{/if}}
      <div class="artist-info">
      <h2>{{this.name}}</h2>
        <a href="/albums/{{this.id}}">Voir les albums</a>
      
      </div>
    </li>
  {{/each}}
</ul>
