<div class="container albums">
    {{#each albums}}
    <div class="album container column">
        <div>
            <img src="{{this.images.1.url}}" alt="">
        </div>
        <div>
            <h2>{{this.name}}</h2>
            <a href="">View album</a>
        </div>
    </div>
    {{/each}}
</div>