<div class="artists">

    <div id="artist">

        {{#each artists}}
            <img src="{{this.images.0.url}}" width="200px">
            <p> {{this.name}} </p>
            <button>
                <a href="/albums/{{this.id}}"> View Albums </a> 
            </button> 
            </br>
        {{/each}}

    </div>
</div>
