<div class="container">
  <div class="sheet">

    <ul>
      {{#each artists}}
      <li>
        <img src="{{images.[0].url}}" alt="{{name}}">
        <h2>{{name}}</h2>
        <a href="/albums/{{id}}">
          <h3>View Albums</h3>
        </a>
      </li>
      {{/each}}
    </ul>
  </div>
</div>