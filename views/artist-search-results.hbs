<div class="row row-cols-3 row-cols-md-3 g-4">
    {{#each artists }}
        <div class="col mt-5 mb-5 text-center">
            <div>
            {{#if images}}
                <img src=" {{images.[1].url}}" alt="{{name}}">
            {{/if}}
        </div>

        <div>
            {{name}}
        </div>

        <div>
            <a href="/albums/{{id}}" class="btn btn-danger">View Albums</a>
        </div>
        </div>
    {{/each}}
</div>





