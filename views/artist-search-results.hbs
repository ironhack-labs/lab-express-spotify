<div class="main">
    <div class="wrapper">
        {{#each items}}
            <div class="content">
                <img src="{{this.images.0.url}}" alt="">
                <h2>{{this.name}}</h2>
                <a href="/albums/{{this.id}}">View Albums</a>
            </div>
        {{/each}}
    </div>
</div>

