<div class="super">
  <a href="/">Home</a>
</div>
<div class="container d-flex flex-wrap">
  {{#each data}}
  <div class="card m-2" style="width: 18rem">
    <img
      src="{{this.images.[0].url}}"
      class="card-img-top"
      alt="{{ this.name }}"
    />
    <div class="card-body">
      <h5 class="card-title">{{ this.name }}</h5>
      <a href="/albums/{{ this.id }}" class="btn btn-primary">View Albums</a>
    </div>
  </div>
  {{/each}}
</div>
