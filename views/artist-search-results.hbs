<h1>List of Reference :</h1>

    <div class="div-ref">
        {{#each dataArr}}
            <section class="ref-summary">
                <img src="{{this.images.[0].url}}">
                <h2>{{this.name}}</h2>
                <a href="/albums/{{this.id}}" ><button>Views Album</button></a>
            </section>
        {{/each}}
    </div>