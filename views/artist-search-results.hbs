<div class="artists-results">
    {{#each artistsList}}
        <div class="card">
            <h5>{{ name }}</h5>
            {{!-- para pintar uma imagem faz image.i --}}
            <img src="{{ images.0.url }}" alt="{{ name }}" width="150px">
            {{!-- link para ver os detalhes do artista, buscado pelo ID --}}
            <a href="/albums/{{ id }}">See albums</a>
        </div>
    {{/each}}
</div>