<h1 class="text-center">Your search results</h1>
<div class="card-group">
{{#each artist}}
<div class="card text-center my-2 mx-4 border">
<img src="{{images.0.url}}" alt="Image of {{name}}" class="card-img-top">
<div class="card-body">
<h2 class="card-title">{{name}}</h2>
<a href="/albums/{{id}}" class="btn btn-primary">View Albums</a>
</div>
</div>
{{/each}}
</div>


