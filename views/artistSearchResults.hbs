<h1>Players view</h1>

<div class="row row-cols-1 row-cols-md-2 g-4">
  {{#each albums}}
    <div class="col">
      <div class="card h-100">
        <img src="#" class="card-img-top" alt="..." />
        <div class="card-body">
          <h5 class="card-title">{{this.name}}</h5>
          <a href="{{this.external_urls.spotify}}">View Album</a>
        </div>
      </div>
    </div>
  {{/each}}
</div>