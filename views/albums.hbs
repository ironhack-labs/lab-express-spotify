<div id="albums">
    <h1>Results</h1>

    <div class="searchResult">
        {{#each albumsArr}}
            <div class="artist">
                <div class="artistImg">
                    <img src="{{this.images.0.url}}" alt="artist img"/>
                </div>
                <div class="artist_name">
                    <h3>{{this.name}}</h3>
                </div>
                <a href="tracks/{{this.id}}">View tracks</a>
            </div>
        {{/each}}
    </div>
</div>