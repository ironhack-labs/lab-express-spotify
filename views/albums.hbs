<div class="albums-container">
    {{#each albums}}
        <div class="albums-container-album">
            <p>{{this.name}}</p>
            <div class="album-image">
                <img src={{this.images.[0].url}}>
            </div>
            <a href="/tracks/{{this.id}}">View Tracks</a>
        </div>
    {{/each}}
</div>
