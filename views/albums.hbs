<h1 class="heading">Albums of {{artistName}}</h1>
{{#each albums}}
  <div class="albumContainer">
    <img class="albumImage" src="{{images.0.url}}" />
    <div class="albumName">
      {{name}}

      <a class="tracksButton" href="/tracks/{{id}}?albumName={{name}}">View Tracks</a>
    </div>
  </div>
{{/each}}