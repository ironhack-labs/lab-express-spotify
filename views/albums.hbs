<div class="container">
    
    {{#each albums}}


    <div class="card" style="width: 18rem;">
        <img src="{{images.[0].url}}" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <a href="/tracks/{{id}}" class="btn btn-primary">Tracks</a>
        </div>
    </div>


    {{/each}}

</div>