<h1>Albums nombre del artista</h1>

<div class="row row-cols-1 row-cols-md-2 g-4">
    {{#each albums}}
    <div class="col">
        <div class="card h-100">
            <img src="{{this.images.0.url}}" class="card-img-top" alt="..." />
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <a href="/tracks/{{this.id}}">View Albums</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>