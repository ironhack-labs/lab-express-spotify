<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Albums</title>
  </head>
  <body>

    <h1>Artist's albums</h1>

    {{#each albums}}
      <p>{{this.name}}</p>
      <img src="{{this.images.0.url}}" alt="artist {{this.name}}" />

    {{/each}}
  </body>
</html>