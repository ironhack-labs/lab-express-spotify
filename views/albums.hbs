{{#each albums}}
<div class="card" style="width: 18rem;">
  <img src="{{images.[0].url}}" class="card-img-top" alt="..." height="150px">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <a href="/tracks/{{id}}" class="btn btn-primary" role="button">View Tracks</a>
  </div>
</div>
{{/each}}
