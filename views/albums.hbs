<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>Albums</h1>
    {{#each albums}}
      <div>
        <h2>{{name}}</h2>
        <img src="{{images.0.url}}" alt="Album Cover" />
        <a href="/tracks/{{id}}">View Tracks</a>
      </div>
    {{/each}}
  </body>
</html>