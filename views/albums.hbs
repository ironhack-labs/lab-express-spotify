<div class="container">
    <div class="row justify-content-center">
        {{#each albumData}}
        <div class="col-3 artist p-3 d-flex flex-column align-items-center">
            <h5>{{name}}</h5>
            <img src="{{images.[0].url}}" alt="">
            <a href="/tracks/{{id}}">View tracks</a>
        </div>
        {{/each}}
    </div>
</div>