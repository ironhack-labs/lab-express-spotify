
{{#if albums}}
<h1 id="artistNameAlbums">Albums for: {{name}}</h1>
<div id="artistsWrap">
 {{#each albums}}
 <div id="artistCard">

    {{#if images.0.url}}
        <img src="{{images.0.url}}" alt="Album image">
    {{/if}}

    <p>{{this.name}}</p>
    <a href="/tracks/{{this.id}}">View Tracks</a>
    
 </div>
 {{/each}}
</div>

{{/if}}