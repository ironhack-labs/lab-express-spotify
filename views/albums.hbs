<div class="container">
    <div class="row">

        {{#each dataAlbum}}
        <div class="col-4">
            <p class="albumsNames">{{name}}</p>
            <img src="{{images.0.url}}" alt="albumes" class="imgAlbums">
            <a href="/tracks/{{id}}" class="btn btn-info buttonsAlbums"> Link to tracks </a>
        </div>
        {{/each}}
    </div>
</div>