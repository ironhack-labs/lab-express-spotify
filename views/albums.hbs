<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link rel='stylesheet' href='/styles/style.css' />
    <title>Albums</title>
  </head>
  <body>
    <div class='div-all-search'>
      {{#each artistAlbums}}
        <div class='search'>
          <img src='{{this.images.1.url}}' alt='' class='search-img' />
          <h1 class='search-heading'>{{this.name}}</h1>
          <a href='/tracks/{{this.id}}' class='search-a'>See tracks</a>
        </div>
      {{/each}}
    </div>

  </body>
</html>