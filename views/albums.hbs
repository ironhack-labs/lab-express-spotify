<section class="album-container">
    {{!-- Loops through albumObj --}}
    <div class = "container">
    {{#each albumObj }}
    <div class="row">
        <div class = "col-sm">
            <div class="album-image">
                <img src="{{this.images.0.url}}" height="300px"></img>
            </div>
            <div class="album-info">
                <p>album info: {{ this.name }}</p>
                <a href='/tracks/{{ this.id }}'><button type="submit">View Tracks</button></a>
            </div>
        </div> 
    </div>
    {{/each }}
    </div>
</section>

