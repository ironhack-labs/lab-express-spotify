{{#each disco}} {{!array}}
<div class="card" style="width: 18rem;">
    <img class="card-img-top" src="{{ img }}" alt="Card image cap">
    <div class="card-body">
        <h5 class="card-title">{{ name }}</h5>
        <a href="/tracks/{{ id }}" class="btn btn-primary">Views Track</a>
    </div>
</div>

{{/each}}