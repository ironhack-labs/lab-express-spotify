<div class="container-cards">

     {{#each albumsResults}}
    <div class="cards">

         <img src="{{this.images.[0].url}}" alt="">
        
         <div class="text-container">
             <h5>{{this.name}}</h5>
             <a href="/tracks/{{this.id}}">View Tracks</a>
         </div>
    </div>
     {{/each}}
     
 </div>