<h1>Albums for {{ albums.0.artists.0.name }} </h1>
<div class="artists-wrapper">
    {{#each albums}}
        <div class="artist">
        <div class="artist-caption">
            <div class="artist-name">{{ this.name }}</div>
        </div>
        {{#if album.images }}
            <img class="artist-image" src="{{this.images.0.url}}" />
        {{else}}
            <img class="artist-image" src="../images/icons8-music-album-100.png" />
        {{/if}}
        </div>
     {{/each}}
</div>