<h3>Album results</h3>

<div class="artists__container">
    {{#each data}}
    <div class="card artists__card" style="width: 18rem;">
        <img src="{{this.images.0.url}}" alt="" style='width:150px'>
        <div class="card-body">
            <h6 class="card-title">{{this.name}}</h6>
            <a href="/tracks/{{this.id}}" class="btn btn-primary">View Tracks</a>
        </div>
    </div>
    {{/each}}
</div>