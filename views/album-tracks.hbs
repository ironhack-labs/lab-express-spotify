<div class="fixed-header">
<h3 id="title">Title</h3>
<h3 id="listen">Listen</h3>

</div>


<section class="content-section">
<ul class="tracks">
  {{#each listOfTtracks}}
    <li style="list-style-type: none;">
      <span class="track-name">{{this.name}}</span>
      {{#if this.preview_url}}
      <audio controls src="{{this.preview_url}}" type="audio/mp3" /></audio>
      {{else}}
      <h5 style="padding-right: 20px;">No Preview found</h5>
      
      {{/if}}
    </li>
    <hr>
  {{/each}}
</ul>

</section>
