<div class="container">

    {{#each artist}}


    <div class="card" style="width: 18rem;">
        <img src="{{images.[0].url}}" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <a href="/albums/{{id}}" class="btn btn-primary">See more</a>
        </div>
    </div>


    {{/each}}

</div>