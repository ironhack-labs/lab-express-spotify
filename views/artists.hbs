<h2>Artists with the name "{{artistSearch}}"</h2>
<div class="result">
  {{#each data.body.artists.items}}
  <div class="resultBox">
    <img src="{{this.images.0.url}}" width="100" alt="{{this.name}}">
    <br>
    {{this.name}}
    <br>
    <a href="/albums/{{this.id}}">View Albums</a>
  </div>
  {{/each}}
</div>