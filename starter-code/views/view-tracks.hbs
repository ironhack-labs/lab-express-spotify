<div class="tracks-container">
    {{#each tracksQuery}}
    <div class="track-box">
    <h3>{{this.name}}</h3>
    <audio
        controls
        src={{this.preview_url}}>
            Your browser does not support the
            <code>audio</code> element.
    </audio>
    </div>
    {{/each}}
</div>