<div class="header">
    <div>Title</div>
    <div>Listen</div>
</div>
<div class="track-wrapper">
    {{#each tracks}}
    <div class="track-container">
        <p>{{this.name}}</p>
        <audio controls src="{{this.preview_url}}">
            Your browser does not support the
            <code>audio</code> element.
        </audio>
    </div>
    {{/each}}
</div>