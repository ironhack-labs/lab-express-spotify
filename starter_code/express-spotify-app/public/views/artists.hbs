<h1>ARTISTA</h1>
<div class="artist-container">
{{#each artists}}
<div class="artist">
<div class="art-img"><img src="{{this.images.0.url}}" alt="artistimg"></div>
<h1 class="artist-title">{{this.name}}</h1>
<button class="btn btn-secondary"><a href="/albums/{{this.id}}">View Albums</a></button>
</div>
{{/each}}
</div>