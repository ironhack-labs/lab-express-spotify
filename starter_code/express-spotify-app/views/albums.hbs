<div class="container">
<div class="row">
   {{#each albums}}
   <div class="col-lg-4 artist-container">
      {{#if this.images}}
      <img src="{{this.images.0.url}}" alt="" srcset="" class="album-img"> {{else}}
      <img src="/images/placeholder.png" alt=""> {{/if}}
      <div class="artist-info-container">
         <p>{{this.name}}</p>
         <a href="/tracks/{{this.id}}">View Tracks</a>
      </div>
   </div>
   {{/each}}
</div>