<h2>Album for: </h2>

<div class="album-container">
    <ul>
        {{#each albumList }} 
            
            <li>
                <img src = {{ this.images.[0].url }} />
                
                <p>{{ this.name }}</p>

                <a href= "/tracks/{{ this.id }}">Views Tracks</a>
            </li>

        {{/each}}
    </ul>
</div>

