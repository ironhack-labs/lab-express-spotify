
  <body>
    <p>albums</p>
    <% if(artistsSearched!==undefined){%>
      <div class="row">
      <% artistsSearched.forEach((artist)=>{ %>
        <div class="col-xs-offset-1 col-xs-3 col-xs-offset-1">
        <div class="album">

        <% if(artist.images.length!==0){ %>
          <div class="row">
          <div class="album-image"><img src=<%= artist.images[0].url%>></img></div></div>

        <%}else{%>
          <div class="row">
          <div class="album-image"><img src="../public/images/blank.png"></img></div></div>

        <%}%>
        <div class="row">
            <div class="album-bottom">
              <div class="row">
              <div class="col-xs-offset-3 col-xs-3">
              <h4><%= artist.name%></h4>
              </div>
              </div>
              <div class="row">
                <div class="col-xs-offset-3 col-xs-3">
              <button><a href=<%='/albums/'+ artist.id%>>View More</a></button>
              </div>
              </div>
              </div>
            </div>
          </div>
          </div>

  <%})%>
  </div>
  <%}%>
</body>
