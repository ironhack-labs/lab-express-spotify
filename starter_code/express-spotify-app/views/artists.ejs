<div id='artistSearchPage'>
  <% data.forEach( (artist) => { %>
    <div class='artistWidget'>
      <h2>
        <%= artist.name %>
      </h2>
      <% if (artist.images[0]) {%>
        <img src="<%=artist.images[0].url%>">
        <%} else {%>
          <img src="http://placekitten.com/g/300/300">
          <%}%>
            <a href="/albums/?artistID=<%=artist.id%>" method="get">View More</a>
    </div>
    <%})%>
</div>