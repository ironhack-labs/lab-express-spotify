<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Spotify</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link rel="stylesheet" href="/stylesheets/style.css">
  </head>
  <body>
    <div class="container">
      <div class="row text-center">
        <ul class="single-item col-md-4">
          <% artists.forEach( e => { %>
            <li class="single-item col-sm-4">
              <% e.images.forEach( i => { %>
                <img alt="" src="<%= i.url %>"/>
              <div class="item-footer">
                <h3><%= e.name %></h3>
                <a class="view-more btn" href="<%= e.id%>">
                  View Albums
                </a>
              </div>
            </li>
            <% }) %>
          <% }) %>
        </ul>
      </div>
    </div>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  </body>
</html>
