<body>
    <a href="/">NEW SEARCH</a>
    <h1>
        <%= searchArtist %>
    </h1>
    <div class="row">
        <% for(i=0; i<3; i++){ %>
            <div class="col-lg-4">
                <img src="<%=artists[i].images[0].url%>">
                <div>
                    <p>
                        <%=artists[i].name%>
                    </p>
                    <a href="/albums/<%=artists[i].id%>">View Albums</a>
                </div>
            </div>
            <%}%>
    </div>
</body>