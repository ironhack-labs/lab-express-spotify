<h1>The result for your search: <%= artists.name%></h1>
<div>
<% artists.forEach((artist) => { %>
  <div>
    <h2><%= artist.name %></h2>
    <% if (artist.images.length > 0) { %>
      <img src="<%= artist.images[0].url %>"></img>
    <% } %>
    <a href="/albums/<%= artist.id %>"><button>View albums</button></a>
  </div>
  <% }) %>
</div>
