    <div class="album py-5 bg-light">
        <div class="container">
            <div class="row">
               {{#each artists}}
                <div class="col-md-4">
                    <div class="card mb-2 shadow-sm image">
                      <img class="card-img-top" src="{{ this.images.0.url }}" onerror="this.onerror=null;this.src='data:image/png;base64,';" alt="Man">
                    </div> 
                     <a href="/albums/{{this.id}}" class="btn btn-sm  btn-outline-secondary" style="width: 300px;" >{{this.name}}</a>
                     
                </div>
              {{/each}}
            </div>
        </div>
    </div>