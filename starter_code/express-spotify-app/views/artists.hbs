<div class="artist-container">
    <ul>
        {{#each artistList }} 
            
            <li>
                {{#if this.images.[0].url}}
                    <img src = {{ this.images.[0].url }} />
                {{else}}
                    <img src = "http://www.designshock.com/wp-content/uploads/2016/04/man-4-400.jpg"/>
                {{/if}}
                
                <p>{{ this.name }}</p>

                <a href= "/albums/{{ this.id }}">Views Albums</a>
            </li>

        {{/each}}
    </ul>
</div>

