<div class="container">
  <div class="row">
    {{#each arr}}
      <div class="card col-md-4">
        <img class="card-img-top" src="{{this.images.[0].url}}" />
        <div class="card-body">
          <h4 class="card-title" style="color: white">{{this.name}}</h4>
          <a href="/albums/{{this.id}}"><button>View Albums</button></a>
        </div>
      </div>
    {{/each}}
  </div>
</div>