
  <!-- Card -->
  <div class="card mb-4">

    <!--Card image-->
    <div class="view overlay">
      {{#if this.images }}
        <img class="card-img-top" src="{{this.images.[0].url}}" alt="{{this.name}}">
      {{else}}
        <img class="card-img-top" src="data:image/jpeg;base64," alt="{{this.name}}">

      {{/if}}
      <a href="#!">
        <div class="mask rgba-white-slight"></div>
      </a>
    </div>

    <!--Card content-->
    <div class="card-body">
      <h4 class="card-title">{{this.name}}</h4>
      <a class="btn btn-primary" href="/albums" role="button">View Albums</a>
    </div>

  </div>
  <!-- Card -->
