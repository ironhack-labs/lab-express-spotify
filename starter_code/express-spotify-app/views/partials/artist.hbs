  <div class="artist">
      {{#if images.[1]}}
        <img class="artist-img-top" src="{{images.[1].url}}" alt="image">
      {{else}}
        <img class="artist-img-top" src="/images/random.jpeg" alt="image">
      {{/if}}
        <div class="artist-body">
          <form action="">
            <div class="menu">
              <h2 class="artist-name">{{name}}</h2>
              <button action="/albums" name="{{name}}">View Albums</button>
            </div>
          </form>
        </div>
  </div>