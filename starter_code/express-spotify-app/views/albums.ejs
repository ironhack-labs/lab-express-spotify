<div class="container">
    <div class="row">
      <% albums.forEach((album) => { %>
        <div class="col-lg-4 artist-container">
          <% if (album.images[0]) { %>
            <img src="<%= album.images[0].url %>" alt="" class="artist-img">
          <% } else { %>
            <img src="http://www.designshock.com/wp-content/uploads/2016/04/man-4-400.jpg" alt="" class="artist-img">
          <% } %>
          <div class="artist-info-container">
            <p><%= album.name %></p>
            <a href="/tracks/<%=album.id %>">View Tracks</a>
          </div>
        </div>
      <% }) %>
    </div>
  </div>