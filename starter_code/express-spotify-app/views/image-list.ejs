<% if(items.length === 0) { %>
	<div class="row">
		<h3>No <% if(typeItem === 'artists') { %>artists <% } else { %>albums <% } %> found for: <%= input %>"</h3>
	</div>
<% } %>
<% for (i=0; i< items.length ; i++) { %>
	<% if(i!==0 && i%4 === 0) { %>
		</div>
	<% } %>
	<% if(i%4 === 0) { %>
		<div class="row">
	<% } %>
	<div class="col-sm-3 text-center item-image-list-container">
		<img src="<%= items[i].image %>" alt="<%= items[i].name %>" class="img-thumbnail"/>
		<h4><%= items[i].name %></h4>
		<a href="/<% if(typeItem === 'artists') { %>albums<% } else { %>tracks<% } %>?itemId=<%= items[i].id %>&itemName=<%= items[i].name %>" class="btn btn-primary">View <% if(typeItem === 'artists') { %>Albums <% } else { %>Tracks <% } %></a>
	</div>
<% } %>
<% if(items.length > 0) { %>
	</div>
<% } %>
		
<%- contentFor('title') %>
<% if(typeItem === 'artists') { %>Artists <% } else { %>Albums <% } %>for: <%= input %>