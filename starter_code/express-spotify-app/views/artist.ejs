
        <h2 class="my-3">
            <span class="text-primary" <%= data.body.artists.items.length %></span>
            <span class="text-dark">Artists matches:</span>
            <span class="text-primary"><%= keyword %></span>
        </h2>
        <div class="card-columns">
        <% data.body.artists.items.forEach( (item) => { 
                if(item.images.length != 0){
                    imageUrl = item.images[0].url;
                }
        %>
            <div class="card">
                <img class="card-img-top" src="<%= imageUrl %>" alt="<%= imageUrl %>">
                <div class="card-body">
                    <h5 class="card-title"><%= item.name %></h5>
                    <p class="card-text"><button><a href="/albums/<%= item.id %>">View Albums</a></button></p>
                </div>
            </div>
        <% }); %>
        </div>
