<div class="container">
  <table class="table" style="margin-top: 20px;">
    <thead>
      <tr>
        <th>Track</th>
        <th>Preview</th>
      </tr>
    </thead>
    <tbody>
      {{#each tracks}}
      <tr>
        <td> {{this.name}} </td>
        {{#if this.preview_url}}
        <td> <audio src="{{this.preview_url}}"></audio> </td>
        {{else}}
        <td> NO PREVIEW AVAILABLE </td>
        {{/if}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>