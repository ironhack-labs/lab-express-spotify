<table class="highlight">
  <thead id="titl">
    <tr>
        <th>Title</th>
        <th>Listen</th>
    </tr>
  </thead>
  <tbody>

      {{#each items as |track|}}
      <tr>
          <td>{{track_number}}. {{name}}</td>
          <td><audio class="uk-width-2-3@m" src="{{preview_url}}" controls></audio></td>
      </tr>

      {{/each}}

  </tbody>
</table>