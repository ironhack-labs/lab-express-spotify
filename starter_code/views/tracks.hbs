<h1> Tracks </h1>

<div class="track-container">
    {{#each tracks}}
    <div class="track-items">
        <div class="tracks">
            <h2>{{this.track_number}}.</h2>
            <h2>{{this.name}}</h2>
        </div>
        <audio controls src="{{preview_url}}"></audio>
    </div>
    {{/each}}
</div>