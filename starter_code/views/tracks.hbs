<main class="main-container">
  <h1>Tracks</h1>
  <div class="album-tracks">
    {{#each tracksObj}}
      <article class="track-container">
        <h5 class="item-name">{{this.name}}</h5>
        <audio
            controls
            src="{{this.preview_url}}">
                Your browser does not support the
                <code>audio</code> element.
        </audio>
      </article>
    {{/each}}
  </div>
  

</main>