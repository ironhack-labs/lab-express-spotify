<div class="col-lg-4 artist-container">
    <img
        {{#if this.images}}
        src="{{this.images.[0].url}}"
        {{else}}
        src="http://www.designshock.com/wp-content/uploads/2016/04/man-4-400.jpg"
        {{/if}}
        alt="" class="artist-img">
    <div class="artist-info-container">
        <p>{{this.name}}</p>
        <a href="/albums/{{this.id}}">View Albums</a>
    </div>
</div>