<div class="card-columns m-4 p-4 my-shadow">
    {{#each myData}}
        <div class="card bg-light border-dark my-shadow p-4">
            <img class="card-img-top" src="{{images.0.url}}" alt="Card image cap">
            <div class="card-body text-dark">
                <h5 class="card-title">{{name}}</h5>
                <p class="card-text">
                <a href="/albums/{{id}}" class="btn btn-danger btn-sm" tabindex="-1" role="button" aria-disabled="true">View Albums</a>
            </div>
        </div>
    {{/each}}
</div>