<div class="container">
    <div class="row">
        {{#each chosen}}
        <div class="col-4">
            <h3>{{ this.name }}</h3>
            <div class="div-album"><img src=" {{this.images.[0].url}}" class="album-pic" width="100" height="100"></div>
            <button><a href="/albums/:{{this.id}}">See Albums</a></button>
        </div>
        {{/each}}
    </div>
</div>



