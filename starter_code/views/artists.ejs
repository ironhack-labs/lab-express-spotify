<div class="artist-container">
  <% artists.forEach( (artist) => { %>
    <div class="artist">
      <h2> <%= artist.name %> </h2>
      <form action="http://localhost:3000/albums/<%= artist.id %>" method="get">
        <input value="View Albums" type="submit"></input>
      </form>
      <% if(artist.images.length > 0) {%>
        <img src=" <%= artist.images[0].url %> "></img>
      <%} else { %>
        <p> No photo. </p>
        <% } %>
        </div>
      <% } ) %>
</div>
