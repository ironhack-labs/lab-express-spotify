<div id="title-container">
  <h2>{{obj.[0].name}}</h2>
</div>
<div class="container">
  {{#each obj}}
  <div class="column">
    <div class="obj-container">
      <img src="{{images.[0].url}}" width=100>
      <div class="text-container">
        <h4>{{name}}</h4>
        <a class="btn" href="/albums/{{id}}">View Albums</a>
      </div>
    </div>
  </div>
  {{/each}}
</div>