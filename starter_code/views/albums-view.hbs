<div class="container row d-flex justify-content-around">
  {{#each items}}
    <div class="col-4 justify-content-center text-center">
      <img class="artist-img" src="{{this.images.0.url}}" alt="Album Image">
      <h1 class="artist-name">{{this.name}}</h1>
      <button><a href="/tracks/{{this.id}}">View Tracks</a></button>
    </div>
  {{/each}}
</div>