<div id="title-container">
  <h2>Albums of: {{obj.[0].artists.[0].name}}</h2>
</div>
<div class="container">
  {{#each obj}}
  <div class="column">
    <div class="obj-container">
      <img src="{{images.[0].url}}">
      <div class="text-container">
        <h4>{{name}}</h4>
        <a class="btn" href="/tracks/{{id}}">View Tracks</a>
      </div>
    </div>
  </div>
  {{/each}}
</div>