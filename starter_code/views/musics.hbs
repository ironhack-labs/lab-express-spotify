
<div class="d-flex flex-column align-items-center">
  <div class="music-header d-flex flex-nowrap align-items-center
    justify-content-between">
    <h2>Title</h2>
    <h2>Listen</h2>
  </div>
  {{#each musics}}
  <div class="music d-flex flex-nowrap align-items-center
    justify-content-between">
    <h4>{{this.name}}</h4>
    <figure>
      <audio
        controls
        src="{{this.preview_url}}">
        Your browser does not support the
        <code>audio</code> element.
      </audio>
    </figure>
  </div>
  {{/each}}
</div>
