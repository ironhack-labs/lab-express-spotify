<h1 class="text-center">Tracks</h1>
{{#each tracksResult}}
<div class="my-5 container">
    <h5>{{this.name}}</h5>
    {{#if this.preview_url}}
        <audio controls>
            <source src="{{this.preview_url}}" type="audio/mp3">
        </audio>
    {{else}}
        <p>Sorry, not found</p> 
    {{/if}}
</div>
{{/each}}