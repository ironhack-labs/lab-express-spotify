{{#each sound}}

<h1>{{this.name}}</h1>

<audio controls>
  <source src="{{this.preview_url}}" type="audio/mp3">
  <source src="{{this.preview_url}}" type="audio/ogg">
  <p>Your browser doesn't support HTML5 audio. Here is
     a <a href="myAudio.mp4">link to the audio</a> instead.</p>
</audio>
{{/each}}
